{
  "max_urls_to_crawl": 50,
  "crawl_timeout_seconds": 5,
  "max_requests_per_second": 10,
  "start_url": "https://books.toscrape.com/",
  "output_folder": "data",
  "output_name": "books",
  "output_file_extension": "json",

  "schema_name": "Person",
  "schema_model_file": "person.go",
  "models_folder_name:": "models",
  "$schema": {
    "name": "John",
    "age": 30,
    "email": "john@example.com",
    "address": {
      "street": "123 Main St",
      "city": "New York",
      "zipcode": "10001"
    }
  }
}
